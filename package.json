{
  "name": "timber",
  "version": "0.1.0",
  "description": "Timber.io node client.",
  "license": "ISC",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/timber/timber-node.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "maintainers": [
    "Ben Johnson <ben@timber.com>",
    "David Antaramian <david@timber.io>",
    "Zach Sherman <zach@timber.io>"
  ],
  "author": {
    "name": "Zach Sherman <zach@timber.io>",
    "url": "https://github.com/zsherman"
  },
  "scripts": {
    "watch": "babel src -d dist --experimental -w",
    "clean": "rimraf dist coverage",
    "lint": "eslint src",
    "ci": "babel test/*.test.js",
    "compile": "babel -d dist src",
    "build": "npm run clean && babel -d dist src",
    "test": "jest --coverage",
    "cover": "npm run compile && babel node_modules/.bin/babel-istanbul cover node_modules/.bin/blue-tape ./dist/*.js --report text-summary",
    "report": "npm run compile && babel node_modules/.bin/babel-istanbul cover node_modules/.bin/blue-tape ./dist/*.js --report html && open ./coverage/index.html",
    "coveralls": "npm run compile && babel node_modules/.bin/babel-istanbul cover node_modules/.bin/blue-tape ./dist/*.js --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postcoveralls": "rimraf coverage",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master"
  },
  "bugs": {
    "url": "https://github.com/timberio/timber-node/issues"
  },
  "keywords": [
    "es6",
    "node",
    "module",
    "timber",
    "logger",
    "logging",
    "structured logging",
    "node logging",
    "express logging",
    "morgan logging",
    "koa logging",
    "framework logging",
    "middleware"
  ],
  "homepage": "https://github.com/timberio/timber-node#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "2.11.4",
    "eslint": "1.8.0",
    "eslint-config-xo": "0.7.1",
    "eslint-plugin-babel": "2.1.1",
    "jest-cli": "^18.1.0",
    "rimraf": "2.4.3",
    "tap-spec": "^4.1.0"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "msgpack": "^1.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": "src/**/*.js"
  }
}
